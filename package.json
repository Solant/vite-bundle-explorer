{
  "name": "vite-bundle-explorer",
  "description": "Bundle analyzer and visualizer tool for Vite",
  "version": "0.3.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && pnpm build:plugin && vite build",
    "build:plugin": "tsdown",
    "format": "prettier src plugin bin vite.config.ts --write",
    "release": "pnpm build && pnpm changelogen --release --push && pnpm publish"
  },
  "homepage": "https://github.com/Solant/vite-bundle-explorer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Solant/vite-bundle-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/Solant/vite-bundle-explorer/issues"
  },
  "keywords": [
    "vite",
    "bundle",
    "explorer",
    "analyzer",
    "visualizer",
    "plugin",
    "size",
    "analysis",
    "optimization",
    "stats",
    "visualization",
    "import-graph",
    "dependency-graph",
    "treemap",
    "rollup",
    "performance",
    "build"
  ],
  "exports": {
    "./plugin": {
      "types": "./dist-plugin/stats-plugin.d.ts",
      "default": "./dist-plugin/stats-plugin.js"
    }
  },
  "files": [
    "dist-ui/index.html",
    "dist-ui/assets",
    "dist-plugin/stats-plugin.js",
    "dist-plugin/stats-plugin.d.ts",
    "bin/index.js"
  ],
  "bin": {
    "vite-bundle-explorer": "./bin/index.js"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/vscode-icons": "^1.2.30",
    "@types/node": "^24.0.13",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/preset-wind3": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.5.0",
    "changelogen": "^0.6.2",
    "echarts": "^5.6.0",
    "prettier": "^3.6.2",
    "reka-ui": "^2.3.2",
    "tsdown": "^0.15.4",
    "typescript": "~5.8.3",
    "unocss": "^66.3.3",
    "vite": "^7.1.7",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.12"
  }
}
